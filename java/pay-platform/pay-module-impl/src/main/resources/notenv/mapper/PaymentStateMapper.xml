<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pay.single.dao.PaymentStateMapper" >

  <resultMap id="BaseResultMap" type="paymentState" >
    <id column="payment_state_id" jdbcType="INTEGER" property="paymentStateId" />
    <result column="state_name" jdbcType="VARCHAR" property="stateName" />
    <result column="state_noet" jdbcType="VARCHAR" property="stateNoet" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="create_user" jdbcType="VARCHAR" property="createUser" />
  </resultMap>
  
  <sql id="Base_Column_List" >
    payment_state_id, state_name, state_noet, create_time, create_user
  </sql>
  
  <select id="findById" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    <include refid="Base_Column_List" />
    from pay_payment_state
    where payment_state_id = #{paymentStateId,jdbcType=INTEGER}
  </select>
  
</mapper>